<Window x:Class="c_1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:c_1"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="0.2*"/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <Button HorizontalAlignment="Left" Height="40" Width="100" Click="LoadAllPhoto">Photo</Button>
        <Button HorizontalAlignment="Right" Height="40" Width="100" Click="AllEncrypt">MD5</Button>
        <Button HorizontalAlignment="Center" Height="40" Width="100" Click="GetJson">JSON</Button>
        <GroupBox Grid.Row="1" Header="Json contact">
            <DataGrid  Name="Json"/>
        </GroupBox>
        <Button HorizontalAlignment="Center" Height="40" Width="100" Click="SaveVideo" Content="VideoAudio" Margin="524,15,169.6,14.6"/>
    </Grid>
</Window>
